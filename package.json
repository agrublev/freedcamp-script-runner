{
    "name": "freedcamp-script-runner",
    "version": "1.0.6",
    "description": "Runs the FcScripts.md file",
    "main": "index.js",
    "repository": "https://github.com/agrublev/freedcamp-script-runner.git",
    "author": "Angel Grablev <agrublev@gmail.com>",
    "license": "MIT",
    "private": false,
    "scripts": {
        "fsr": "node src/index.js",
        "predeploy": "echo '52'; prettier src --write",
        "deploy": "run-s version pub",
        "version": "npm version patch",
        "pub": "npm publish",
        "standard": "standardx",
        "prepublish": "run-s pretty",
        "test": "xo",
        "pretty": "prettier src/*.js --write",
        "eslint": "eslint src/*.js --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/*.{js,jsx}": [
            "prettier --write",
            "git add"
        ]
    },
    "bin": {
        "fsr": "./index.js",
        "fs": "./src/index.js"
    },
    "files": [
        "!node_modules/**",
        "src/**/*",
        "lib/**/*"
    ],
    "dependencies": {
        "vorpal": "^1.12.0",
        "yargs": "^13.2.2"
    },
    "devDependencies": {
        "colors": "^0.6.2",
        "babel-eslint": "^10.0.1",
        "chalk": "^2.4.2",
        "cli-table2": "^0.2.0",
        "execa": "^1.0.0",
        "fancy-log": "^1.3.3",
        "get-stream": "^5.0.0",
        "inquirer": "^6.2.2",
        "joycon": "^2.2.4",
        "luxon": "^0.1.0",
        "markdown-it": "^8.4.2",
        "md-2-json": "^1.0.5",
        "micromatch": "^3.1.10",
        "moment": "^2.24.0",
        "prepend-file": "^1.3.1",
        "require-from-string": "^2.0.2",
        "rexrex": "^1.3.0",
        "level": "^0.18.0",
        "minimist": "^0.1.0",
        "tap": "^0.4.9",
        "blessed": "^0.1.81",
        "changed-git-files": "^0.0.1",
        "conf": "^3.0.0",
        "crypto-js": "^3.1.9-1",
        "eslint": "^5.15.3",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^3.1.0",
        "eslint-config-react-app": "^3.0.6",
        "eslint-config-xo": "^0.26.0",
        "eslint-config-xo-react": "^0.19.0",
        "eslint-find-rules": "^3.3.1",
        "eslint-plugin-ava": "^6.0.0",
        "eslint-plugin-flowtype": "^3.2.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-markdown": "^1.0.0",
        "eslint-plugin-monorepo": "^0.2.1",
        "eslint-plugin-no-use-extend-native": "^0.4.0",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-react": "^7.11.1",
        "eslint-plugin-unicorn": "^7.1.0",
        "figures": "^2.0.0",
        "first-run": "^1.2.0",
        "fkill": "^6.0.0",
        "fs-extra": "^7.0.1",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.5",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.16.4",
        "standard": "^10.0.3",
        "standardx": "^3.0.1",
        "traverse": "^0.6.6",
        "xo": "^0.24.0"
    }
}
