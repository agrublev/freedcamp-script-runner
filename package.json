{
	"name": "fscripts",
	"version": "3.7.17",
	"description": "Runs the fscripts.md file",
	"main": "./index.js",
	"scripts": {
		"fsr": "node index.js",
		"release": "run-s release:bump release:build release:publish",
		"release:build": "parcel build index.js --target node --no-cache",
		"release:bump": "yarn fsr bump",
		"release:publish": "node lib/release/publish.js",
		"run-s": "node index.js run-s"
	},
	"fscripts": {
		"encryptedFiles": [
			"config.json"
		],
		"ignore-upgrade": [
			"micromatch"
		]
	},
	"repository": "https://github.com/agrublev/freedcamp-script-runner.git",
	"private": false,
	"license": "MIT",
	"author": "Angel Grablev <agrublev@gmail.com>",
	"bin": {
		"fsr": "./index.js"
	},
	"dependencies": {
		"better-md-2-json": "^1.0.6",
		"boxen": "^4.1.0",
		"chalk": "^2.4.2",
		"conf": "^6.0.1",
		"cross-spawn": "^7.0.0",
		"crypto-js": "^3.1.9-1",
		"detect-indent": "latest",
		"enquirer": "^2.3.2",
		"fancy-log": "^1.3.3",
		"fs-extra": "^8.1.0",
		"inquirer": "^7.0.0",
		"joycon": "^2.2.5",
		"markdown-it": "^10.0.0",
		"markdown-toc": "^1.2.0",
		"marked": "^0.7.0",
		"md-2-json": "^1.0.6",
		"micromatch": "^4.0.2",
		"moment": "^2.24.0",
		"prepend-file": "^1.3.1",
		"prettier": "^1.18.2",
		"pretty-ms": "^5.0.0",
		"require-from-string": "^2.0.2",
		"rexrex": "^1.3.0",
		"simple-git": "^1.126.0",
		"sort-object-keys": "^1.1.2",
		"versiony": "^2.0.1",
		"yargs": "^14.0.0"
	},
	"devDependencies": {
		"fkill-cli": "^5.2.0",
		"git-changed-files": "^1.0.0",
		"git-release-notes": "^5.0.0",
