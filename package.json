{
	"name": "fscr",
	"version": "2.0.2",
	"description": "Runs the fscripts.md file",
	"main": "./dist/index.js",
	"scripts": {
		"ab:run-s": "parcel build runall/lib/index.js --target node  --no-cache --no-source-maps -o lib.js",
		"b:run-s": "babel runall/lib/index.js --ignore 'node_modules' -q --no-stdin -o lib.js",
		"prebuild": "rimraf dist/*",
		"build": "run-p build:index build:lib",
		"build:index": "babel index.js --ignore 'node_modules' -q --no-stdin --out-file dist/index.js",
		"build:lib": "babel lib --ignore 'node_modules' -q --no-stdin --out-dir dist/lib",
		"fsr": "node dist/index.js",
		"release": "run-s release:bump release:clean release:build release:publish",
		"release:build": "parcel build index.js --target node --no-cache --bundle-node-modules --no-source-maps",
		"release:bump": "yarn fsr bump",
		"release:clean": "rimraf dist",
		"release:combined": "np --any-branch",
		"release:publish": "node lib/release/publish.js",
		"start": "run-s build start:run",
		"start:basic": "babel-node index.js",
		"start:build": "babel-node index.js --ignore 'node_modules' -q --no-stdin",
		"start:run": "node dist/index.js",
		"start:watch": "nodemon --watch lib --watch index.js --ext js --exec babel-node index.js --ignore 'node_modules' -q --no-stdin",
		"test1": "echo 'yolo' && sleep 2 && echo 1",
		"test2": "echo '2yolo' && sleep 2 && echo 22",
		"zb:run-s": "parcel build runall/run-s/index.js --target node  --bundle-node-modules --no-cache --no-source-maps -o runs.js"
	},
	"fscripts": {
		"encryptedFiles": [
			"config.json"
		],
		"ignore-upgrade": [
			"micromatch"
		]
	},
	"repository": "https://github.com/agrublev/freedcamp-script-runner.git",
	"private": false,
	"license": "MIT",
	"author": "Angel Grablev <agrublev@gmail.com>",
	"files": [
		"dist",
		"index.js",
		"bin"
	],
	"bin": {
		"fscr": "./bin",
		"fsr": "./bin"
	},
