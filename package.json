{
	"name": "fcr",
	"version": "0.1.9",
	"description": "Freedcamp task runner with Markdown. Great",
	"repository": {
		"url": "agrublev/freedcamp-script-runner",
		"type": "git"
	},
	"main": "bin/run.js",
	"bin": {
		"fcr": "bin/run.js"
	},
	"files": [
		"bin",
		"lib"
	],
	"scripts": {
		"r": "node bin/run.js",
		"build": "parcel build lib/index.js --out-dir bin --no-cache --target node --no-source-maps",
		"build2": "parcel build bin/testme.js --out-dir bin -o run.js --target node  --no-cache --no-source-maps",
		"build3": "webpack --config webpack.dev.js"
	},
	"author": "egoist <0x142857@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"cac": "^6.4.2",
		"chalk": "^2.4.1",
		"cross-spawn": "^6.0.5",
		"doctoc": "^1.3.1",
		"fancy-log": "^1.3.2",
		"inquirer": "^6.2.2",
		"joycon": "^2.2.4",
		"markdown-it": "^8.4.1",
		"markdown-toc": "^1.2.0",
		"md-2-json": "^1.0.5",
		"micromatch": "^3.1.10",
		"prepend-file": "^1.3.1",
		"require-from-string": "^2.0.2",
		"rexrex": "^1.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@babel/node": "^7.2.2",
		"@babel/plugin-transform-runtime": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"ava": "^1.2.1",
		"babel-loader": "^8.0.5",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"eslint": "^5.15.0",
		"eslint-config-prettier": "^4.1.0",
		"eslint-config-rem": "^4.0.0",
		"eslint-config-xo": "^0.26.0",
		"eslint-plugin-prettier": "^3.0.1",
		"execa": "^1.0.0",
		"husky": "^1.0.0-rc.8",
		"lint-staged": "^8.1.5",
		"prettier": "^1.13.3",
		"uglifyjs-webpack-plugin": "^2.1.2",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.2.3"
	},
	"eslintConfig": {
		"extends": [
			"xo",
			"rem",
			"plugin:prettier/recommended"
		],
		"rules": {
			"unicorn/filename-case": "off",
			"no-await-in-loop": "off"
		}
	}
}
