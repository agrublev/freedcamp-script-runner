{
    "name": "freedcamp-script-runner",
    "version": "1.1.12",
    "description": "The ultimate project executor!",
    "main": "index.js",
    "repository": "https://github.com/agrublev/freedcamp-script-runner.git",
    "author": "Angel Grablev <agrublev@gmail.com>",
    "license": "MIT",
    "private": false,
    "oclif": {
        "commands": "./src/commands",
        "bin": "fsr",
        "hooks": {
            "init": "./src/init.js"
        },
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-warn-if-update-available",
            "@oclif/plugin-autocomplete",
            "@oclif/plugin-update"
        ],
        "warn-if-update-available": {
            "timeoutInDays": 0,
            "message": "Please consider upgrading!\n<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>.\nnpm i -g freedcamp-script-runner@<%= chalk.greenBright(latest) %>"
        }
    },
    "scripts": {
        "fsr": "node bin/run",
        "fsr:d": "DEBUG=* node bin/run",
        "predeploy": "echo '52'; prettier src --write",
        "deploy": "run-s pack version pub",
        "version": "node publish.js",
        "pub": "npm publish",
        "pret": "run-s pretty",
        "test": "xo",
        "pretty": "prettier src/*.js --write",
        "eslint": "eslint src/*.js --fix",
        "pack": "run-s prepack postpack",
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint .",
        "prepack": "oclif-dev manifest && oclif-dev readme",
        "o:test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
        "o:version": "oclif-dev readme && git add README.md"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/*.{js,jsx}": [
            "prettier --write",
            "git add"
        ]
    },
    "bin": {
        "fsr": "./bin/run",
        "fzr": "./index.js",
        "fs": "./src/index.js"
    },
    "files": [
        "/bin",
        "!/node_modules",
        "/src",
        "/lib"
    ],
    "dependencies": {
        "@oclif/command": "^1",
        "@oclif/config": "^1",
        "@oclif/plugin-autocomplete": "^0.1.0",
        "@oclif/plugin-help": "^2",
        "@oclif/plugin-update": "^1.3.9",
        "@oclif/plugin-warn-if-update-available": "^1.7.0",
        "blessed": "^0.1.81",
        "chalk": "^2.4.2",
        "changed-git-files": "^0.0.1",
        "cli-ux": "^5.2.1",
        "colors": "^0.6.2",
        "conf": "^3.0.0",
        "crypto-js": "^3.1.9-1",
        "figures": "^2.0.0",
        "first-run": "^1.2.0",
        "fkill": "^6.0.0",
        "fs-extra": "^7.0.1",
        "globby": "^8",
        "inquirer": "^6.2.2",
        "joycon": "^2.2.4",
        "luxon": "^0.1.0",
        "markdown-it": "^8.4.2",
        "md-2-json": "^1.0.5",
        "minimist": "^0.1.0",
        "moment": "^2.24.0",
        "node-notifier": "^5.4.0",
        "prepend-file": "^1.3.1",
        "require-from-string": "^2.0.2",
        "rexrex": "^1.3.0",
        "tap": "^0.4.9",
        "traverse": "^0.6.6",
        "update-notifier": "^2.5.0",
        "vorpal": "^1.12.0",
        "yargs": "^13.2.2"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1",
        "@oclif/test": "^1",
        "babel-eslint": "^10.0.1",
        "chai": "^4",
        "cli-table2": "^0.2.0",
        "eslint": "^5.15.3",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^3.1.0",
        "eslint-config-react-app": "^3.0.6",
        "eslint-config-xo": "^0.26.0",
        "eslint-config-xo-react": "^0.19.0",
        "eslint-find-rules": "^3.3.1",
        "eslint-plugin-ava": "^6.0.0",
        "eslint-plugin-flowtype": "^3.2.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-markdown": "^1.0.0",
        "eslint-plugin-monorepo": "^0.2.1",
        "eslint-plugin-no-use-extend-native": "^0.4.0",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-react": "^7.11.1",
        "eslint-plugin-unicorn": "^7.1.0",
        "execa": "^1.0.0",
        "fancy-log": "^1.3.3",
        "get-stream": "^5.0.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.5",
        "micromatch": "^3.1.10",
        "mocha": "^5",
        "npm-run-all": "^4.1.5",
        "npm-version-bump": "^1.0.0",
        "nyc": "^13",
        "prettier": "^1.16.4",
        "xo": "^0.24.0"
    }
}
